{
  "extends": [
    "config:base"
  ],
  "packageRules": [
    {
      "matchManagers": ["github-actions"],
      "groupName": "github-actions",
      "groupSlug": "github-actions",
      "schedule": ["at any time"]
    }
  ],
  "github-actions": {
    "enabled": true
  },
  "labels": ["dependencies"],
  "prHourlyLimit": 2,
  "prConcurrentLimit": 10
}